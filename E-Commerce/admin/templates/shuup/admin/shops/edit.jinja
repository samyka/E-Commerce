{% extends "E-Commerce/admin/base.jinja" %}
{% from "E-Commerce/admin/macros/general.jinja" import content_with_sidebar %}

{% block body_class %}E-Commerce-details{% endblock %}
{% block content %}
    {% call content_with_sidebar(content_id="shop_form") %}
        <form method="post" id="shop_form" novalidate>
            {% csrf_token %}
            {% for form_def in form.form_defs.values() %}
                {% include form_def.template_name with context %}
            {% endfor %}
        </form>
    {% endcall %}
{% endblock %}
